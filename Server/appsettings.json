{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ldap": {
    "url": "ldap.uae",
    "bindDn": "CN=user,OU=branch,DC=ensate,DC=uae,DC=ac,DC=ma",
    "bindCredentials": "{{PASSWORD}}",
    "searchBase": "DC=ensate,DC=uae,DC=ac,DC=ma",
    "searchFilter": "(&(objectClass=inetOrgPerson)(mail={0}))"
  }
}
