{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ldap": {
    "url": "ldap.uae",
    "bindDn": "CN=Admin, OU=roles, DC=ensate,DC=uae,DC=ac,DC=ma",
    "bindCredentials": "{SSHA}{{PASSWORD_TO_FILLED_IN}}",
    "searchBase": "DC=ensate,DC=uae,DC=ac,DC=ma",
    "searchFilter": "(&(|(objectClass=organizationalRole)(objectClass=inetOrgPerson))(mail={0}))"
  }
}
